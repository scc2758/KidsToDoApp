<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kidstodoapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CreateTrophyActivity"
            android:label="@string/title_activity_create_trophy"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".PickImage" />
        <activity android:name=".TrophyActivity" />
        <activity android:name=".AddTrophy" />
        <activity android:name=".EditTrophy" />
        <activity android:name=".TrophyCase" />
        <activity android:name=".CreateToDoEntryActivity" />
        <activity
            android:name=".ToDoEntryActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity> <!-- <activity android:name=".EditEvents" /> -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>